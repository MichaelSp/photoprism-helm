statefulset matches the snapshot:
  1: |
    apiVersion: apps/v1
    kind: StatefulSet
    metadata:
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: photoprism
        app.kubernetes.io/version: 230625-ce
        helm.sh/chart: photoprism-0.2.1
      name: RELEASE-NAME-photoprism-set
    spec:
      replicas: 1
      selector:
        matchLabels:
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: photoprism
      serviceName: RELEASE-NAME-photoprism
      template:
        metadata:
          labels:
            app.kubernetes.io/instance: RELEASE-NAME
            app.kubernetes.io/name: photoprism
        spec:
          containers:
            - env:
                - name: PHOTOPRISM_HTTP_HOST
                  value: 0.0.0.0
                - name: PHOTOPRISM_HTTP_PORT
                  value: "2342"
              envFrom:
                - configMapRef:
                    name: RELEASE-NAME-photoprism-env
                - configMapRef:
                    name: storage
                - secretRef:
                    name: RELEASE-NAME-photoprism-credentials
              image: photoprism/photoprism:231011-ce
              imagePullPolicy: IfNotPresent
              livenessProbe:
                httpGet:
                  path: /
                  port: http
              name: photoprism
              ports:
                - containerPort: 2342
                  name: http
              readinessProbe:
                httpGet:
                  path: /
                  port: http
              resources: {}
              volumeMounts:
                - mountPath: /photoprism/originals
                  name: originals
                  subPath: photos/family
                - mountPath: /photoprism/storage
                  name: storage
          securityContext: {}
          terminationGracePeriodSeconds: 5
